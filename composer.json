{
    "name": "nick-kh/laravel-components",
    "description": "Shared Laravel Blade components",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "NickKh\\Components\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NickKh\\Components\\ComponentsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
